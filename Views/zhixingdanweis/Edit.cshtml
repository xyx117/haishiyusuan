@model yusuanxiangmu.Models.zhixingdanwei

@{
    ViewBag.Title = "Edit";
}

<div style="padding-bottom:5px">
    当前位置：| &gt;参数设置  &gt; 执行单位设置 &gt;编辑执行单位
    <br />
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()    
    <div class="form-horizontal">
        @*<h4>zhixingdanwei</h4>*@
        <div class="form-group">
            <div class="col-md-5"></div>
            <div class="col-md-2">
                <h3>编辑执行单位</h3>
            </div>
            <div class="col-md-5"></div>
        </div>

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.mingcheng)

        <div class="form-group">
            @Html.LabelFor(model => model.mingcheng, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.mingcheng, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.mingcheng, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>            
}
<div>
    @Html.ActionLink("返回", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
