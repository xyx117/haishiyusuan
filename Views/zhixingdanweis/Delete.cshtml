@model yusuanxiangmu.Models.zhixingdanwei

@{
    ViewBag.Title = "删除";
}

<div style="padding-bottom:5px">
    当前位置：| &gt;参数设置  &gt; 执行单位设置 &gt;删除执行单位
    <br />
</div>

<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-5"></div>

        <div class="col-md-2">
            <h3>删除执行单位</h3>
        </div>
        <div class="col-md-5"></div>
    </div>

    <hr />

    <div class="form-group">
        <div class="col-md-2"></div>
        @Html.LabelFor(model => model.mingcheng, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.TextBoxFor(model => model.mingcheng, new { @class = "form-control", @readonly = "readonly" })
            @Html.ValidationMessageFor(model => model.mingcheng, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group   has-error">
        <div class="col-md-4"></div>
        <div class="col-md-8">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="form-actions no-color">
                    <button type="submit" class="btn btn-primary">删除</button>
                    <button type="button" class="btn btn-primary" onclick="window.location.href = '@Url.Action("Index", "zhixingdanweis")'">返 回</button>
                </div>
            }
        </div>
    </div>

</div>
